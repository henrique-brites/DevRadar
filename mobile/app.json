{
  "expo": {
    "name": "Buscar Devs",
    "slug": "BuscaDevs",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#12B6E0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps":{
          "apiKey": "*"
        }   
      },
      "package": "com.devpampa.buscadevs",
      "adaptiveIcon": {
        "backgroundColor": "#12B6E0",
        "foregroundImage": "./assets/icon.png"
      }
    }
  }
}
